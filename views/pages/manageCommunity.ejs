  <script src="javascript/notie.min.js"></script>
  <script src="javascript/CommunityHelper.js"></script>
  <script src="javascript/ManageCommunity.js"></script> -->
  
  <script>
  var selector = '.nav li';
  $(selector).removeClass('active');
  $('#community-panel-nav').addClass('active');

function UserList()
{
  $.post('/manageCommunity', { name: "<%= data[0].name %>",list:"user" }, 
    function(returnedData){
         window.location.href="/manageCommunity";
},'json').fail(function(){
      console.log("error");
});
}

function AdminList()
{
  $.post('/manageCommunity', { name: "<%= data[0].name %>",list:"admin" }, 
    function(returnedData){
         window.location.href="/manageCommunity";
}).fail(function(){
      console.log("error");
});
}

function RequestList()
{
  $.post('/manageCommunity', { name: "<%= data[0].name %>",list:"request" }, 
    function(returnedData){
      window.location.href="/manageCommunity";
},'json').fail(function(){
      console.log("error");
});
}

function InviteList()
{
  $.post('/manageCommunity', { name: "<%= data[0].name %>",list:"invite" }, 
    function(returnedData){
      window.location.href="/manageCommunity";
},'json').fail(function(){
      console.log("error");
});
}

  </script>

<footer>
  <% include ../partials/footer %>
</footer>

</body>
</html>
    