    <body>
        <%- include('../partials/header', {post: 1,name:d[0].username,data:d}); %>
        <% include ../partials/sidebar %>
        <div id="page-content-wrapper" style="padding:0;margin-top:60px"> <!--Margin Top is new-->
          <div class="container-fluid page-content-div" style="padding:0">
             <div class="row">
                <div class="col-lg-12 scoll-possible" style="padding:0">

                <div class="col-sm-12" style="padding-top: 5%">
                        <nav class="navbar navbar-expand-lg navbar-light bg-light">
                                <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                                    <li class="nav-item active">
                                            <button type="button" class="btn btn-primary">user list</button>
                                      </li>
                                      </ul>
                                      
                             <ul class="navbar-nav "  class="pagination pull-right">  
                                    <li class="nav-item active">
                                            <form action="/userList" method="GET"><button type="submit" class="btn btn-secondary">Refresh</button></form>
                                      </li>
                                      <li class="nav-item active" style="padding-left: 5%;">
                                            <div class="dropdown">
                                              <form action="/userList" method="POST">
                                                    <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                      <%= option %>
                                                    </button>
                                                    <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                                      <input class="dropdown-item" id="p" name="p" value="All" type="submit" />
                                                      <input class="dropdown-item" id="p" name="p" value="Pending" type="submit" />
                                                      <input class="dropdown-item" id="p" name="p" value="Confirmed" type="submit"/>
                                                      <!-- <input class="dropdown-item" id="submit" value="go"  type="submit"/> -->
                                                    </div>
                                                    </form>
                                                  </div>
                                      </li>
                                      <li class="nav-item active" style="padding-left: 5%;">
                                          <div class="dropdown">
                                            <form action="/userList" method="POST">
                                                  <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <%= role %>
                                                  </button>
                                                  <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                                    <input class="dropdown-item" id="r" name="r" value="All" type="submit" />
                                                    <input class="dropdown-item" id="r" name="r" value="Admins" type="submit" />
                                                    <input class="dropdown-item" id="r" name="r" value="User" type="submit"/>
                                                    <input class="dropdown-item" id="r" name="r" value="Community Builder" type="submit"/>
                                                    <!-- <input class="dropdown-item" id="submit" value="go"  type="submit"/> -->
                                                  </div>
                                                  </form>
                                                </div>
                                    </li>
                             </ul>
                        </nav>
                        <!--khatam-->
                        <table id="mytable" class="display" style="width:100%">
                          <thead>
                              <tr>
                                  <th>Email</th>
                                  <th>Phone</th>
                                  <th>City</th>
                                  <th>Status</th>
                                  <th>Role</th>
                                  <th>Actions</th>
                              </tr>
                          </thead>
                          <tbody>
                          <% for(var i=0;i<data.length;i++) { %>

                              <tr>
                                  <td><%= data[i].username %></td>
                                  <td><%= data[i].phone %></td>
                                  <td><%= data[i].city %></td>
                                  <td><%= data[i].username %></td>
<% if(data[i].post == 1) { var p="Admin"; } else if(data[i].post == 2) {var p="Community Builder";} else {var p="User";} %>
                                  <td><%= p %></td>
                                  <% if(data[i].post != 1) { %> 
                                    <td>
                                      <a href="/update?q=<%= data[i].email %>"><i class="glyphicon glyphicon-pencil" aria-hidden="true" style="font-size:25px;"></i></a>
                                      <a href="/remove?q=<%= data[i].email %>"><i class="glyphicon glyphicon-remove" aria-hidden="true" style="font-size:25px; padding-left:0.5em;"></i></a>
                                      <a href="/update?q=<%= data[i].email %>"><i class="glyphicon glyphicon-ok-sign" aria-hidden="true" style="font-size:25px; padding-left: 0.5em"></i></a>
                                    </td>   
                                  <% } %>
                              </tr>
                              <% } %>
                          </tbody>
                      </table>
                    
                </div>
            </div>
            </div>
            </div>
            </div>
            
            <footer>
                    <% include ../partials/footer %>
                </footer>
                
                <script>
                    $(document).ready( function () {
              $('#mytable').DataTable();
              } );
                    </script> 

